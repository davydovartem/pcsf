{
  "name": "pcsf",
  "displayName": "PCSF syntax highlighting for Bootfrost prover",
  "description": "Positively Constructed Standardized Formulas (PCSF) file syntax highlighting for Bootfrost prover",
  "version": "0.1.0",
  "engines": {
    "vscode": "^1.97.0"
  },
  "categories": [
    "Programming Languages",
    "Automated theorem provers",
    "First-order logics languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "pcsf",
        "aliases": [
          "BootfrostPCSF",
          "pcsf"
        ],
        "extensions": [
          ".pcsf"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "pcsf",
        "scopeName": "source.pcsf",
        "path": "./syntaxes/pcsf.tmLanguage.json"
      }
    ],
    "themes": [
      {
        "label": "PCSF Theme",
        "uiTheme": "dark-modern",
        "path": "./pcsf-color-theme.json"
      }
    ],
    "commands": [
      {
        "command": "workbench.preferredColorTheme",
        "title": "PCSF Theme"
      }
    ]
  }
}